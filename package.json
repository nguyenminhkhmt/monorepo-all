{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf **/node_modules && rm -rf **/.turbo && rm -rf **/dist && rm -rf **/.expo && rm -rf **/.next && rm -rf **/.cache",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "list-packages": "pnpm list -r",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "clsx": "^2.1.1",
    "prettier": "^3.1.1",
    "tailwind-merge": "^3.0.1",
    "turbo": "^2.4.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "with-react-native-web",
  "packageManager": "pnpm@9.15.4"
}
